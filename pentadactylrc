"1.0b8pre--default

loadplugins '\.(js|penta)$'
group user
map -count -modes=n,v ; <count>:
set guioptions=T
set showtabline=multitab

command! -nargs=+ g open g <args>
map ; :
map <C-h> :tabprevious<Return>
map <C-l> :tabnext<Return>
map J <C-d>
map K <C-u>

" unset titlestring
set titlestring=

" hide dotted border at top
" highlight ContentSeparator display: none;


" vim: set ft=pentdactyl:
