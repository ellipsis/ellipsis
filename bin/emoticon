faces[5]='(-_-メ)'
faces[6]='ヾ(･_･ )'
faces[7]='(｡･_･｡)'
faces[8]='（￣～￣）'
faces[9]='( ´ ▽ ` )'
faces[10]='（；_・）'
faces[11]='（￣へ￣）'
faces[12]='(;´ρ`)'
faces[13]='☆⌒(＞。≪)'
faces[14]='(´・｀)'
faces[15]='(O_O；)'
faces[16]='(。_゜)'
faces[17]='(*・_・)ノ⌒*'
faces[18]='(?・・)σ '
faces[19]='(ﾟｰﾟ)(｡_｡)'
faces[20]='ヾ(-_-;)'

echo $faces[$[${RANDOM}%20]]
